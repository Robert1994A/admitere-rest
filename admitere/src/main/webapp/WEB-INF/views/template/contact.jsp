<div class="panel panel-default">
	<div class="panel-heading">
		<h4 class="panel-title">Contact</h4>
	</div>

	<div class="panel-body">
		<div class="row">
			<div class="col-md-10 col-md-offset-1">
				<form class="widget-contact-form" action="" method="post">
					<div class="row">
						<div class="form-group col-sm-6">
							<label>Name</label> <input type="text"
								class="form-control form-white placeholder"
								placeholder="Enter your Name">
						</div>
						<div class="form-group col-sm-6">
							<label>Email</label> <input type="email" aria-required="true"
								class="form-control form-white placeholder"
								placeholder="Enter your Email">
						</div>
					</div>
					<div class="row">
						<div class="form-group col-sm-12">
							<label>Your Subject</label> <input type="text"
								class="form-control form-white placeholder "
								placeholder="Subject...">
						</div>
					</div>
					<div class="form-group">
						<label>Message</label>
						<textarea rows="5" class="form-control form-white placeholder"
							placeholder="Enter your Message"></textarea>
					</div>
					<button class="btn btn-default" type="submit" id="form-submit">
						<i class="fa fa-paper-plane"></i>&nbsp;Send message
					</button>
				</form>
			</div>
		</div>
	</div>
</div>